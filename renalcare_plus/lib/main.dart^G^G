import 'package:flutter/material.dart';

void main() {
  runApp(RenalCareApp());
}

class RenalCareApp extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      title: 'RenalCare Plus',
      theme: ThemeData(primarySwatch: Colors.teal),
      home: HomeScreen(),
    );
  }
}

class HomeScreen extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(title: Text('RenalCare Plus')),
      body: Center(child: Text('Welcome to RenalCare Plus')),
    );
  }
}
